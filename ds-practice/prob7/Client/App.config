<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application>
      <client>
        <wellknown
            type="Library.Catalog, Catalog"
            url="http://localhost:12345/Catalog.rem"
          />
      </client>
      <channels>
        <channel ref="http">
